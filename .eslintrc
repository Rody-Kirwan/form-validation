{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "semi": 2,
    "eqeqeq": 2,
    "no-multi-spaces": 1,
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    "react/prop-types": 2
  }
}